                        <div class="container text-center">
                            <button class="btn col-lg-5 col-md-5 btn-primary" type="submit">Enviar</button>
                            <button type="submit" class="btn btn-secondary col-lg-5 col-md-5" data-dismiss="modal">Cerrar</button>
                        </div>
                    </form>
                </div>

            </div>
        </div>
