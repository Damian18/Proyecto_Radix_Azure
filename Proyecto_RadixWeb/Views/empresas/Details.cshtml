@model Proyecto_RadixWeb.Models.empresas


@{
    ViewBag.Title = "Details";
}

<div class="content-wrapper">
    <div class="container-fluid">
        <div class="card mb-3">
            <div class="card-header">
                <i class="fa fa-table"></i> Detalles de empresas
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <h4>empresas</h4>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Emp_Nom)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Emp_Nom)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Emp_Cant)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Emp_Cant)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Emp_Estado)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Emp_Estado)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Emp_Dir)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Emp_Dir)
                        </dd>

                    </dl>
                </div>
                <p>
                    @Html.ActionLink("Edit", "Edit", new { id = Model.Emp_Id }) |
                    @Html.ActionLink("Back to List", "Index")
                </p>
            </div>
        </div>
        <div class="card-footer small text-muted">Updated yesterday</div>
    </div>
</div>
